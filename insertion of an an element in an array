[8:48 am, 22/12/2023] Godwin: #include<stdio.h>
int main()
{
    int a[30],n,key,i,pos;
    scanf("%d",&n);
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    scanf("%d",&key);
    scanf("%d",&pos);
    for(i=n;i>pos;i--)
    {
        a[i]=a[i-1];
    }
    a[pos]=key;
    for(i=0;i<n;i++)
    {
        printf("%d ",a[i]);
    }
}
